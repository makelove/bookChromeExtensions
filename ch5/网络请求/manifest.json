{
    "manifest_version": 2,
    "name": "网络请求",
    "version": "1.0",
    "description": "网络请求desc",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "*://*/*"
    ]
}