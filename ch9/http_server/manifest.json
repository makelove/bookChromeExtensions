{
    "app": {
        "background": {
            "scripts": ["tcp.js", "tcpServer.js", "background.js"]
        }
    },
    "manifest_version": 2,
    "name": "HTTP Server",
    "version": "1.0",
    "description": "An HTTP server.",
    "icons": {
        "128": "http_server.png"
    },
    "sockets": {
        "tcp": {
            "connect": "*"
        },
        "tcpServer": {
            "listen": ":80"
        }
    }
}